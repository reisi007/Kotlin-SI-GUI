<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.reisisoft.sigui.ui.OptionUIController">
    <children>
        <VBox alignment="BOTTOM_CENTER" layoutX="0.0" prefHeight="400.0" prefWidth="200.0" spacing="7.0">
            <children>
                <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="7.0" VBox.vgrow="ALWAYS"/>
                <ButtonBar prefHeight="40.0" prefWidth="200.0">
                    <buttons>
                        <Button fx:id="closeButton" mnemonicParsing="false" onAction="#onCloseRequested"
                                text="%options.button.close"/>
                    </buttons>
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                </ButtonBar>
            </children>
        </VBox>
    </children>
</AnchorPane>